{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "number"
    },
    "currency": {
      "type": "string",
      "minLength": 3,
      "maxLength": 3
    },
    "pricePerKwh": {
      "type": "number"
    },
    "pricePerMin": {
      "type": "number"
    },
    "pricePerSession": {
      "type": "number"
    },
    "paymentFee": {
      "type": "number"
    },
    "authorizationAmount": {
      "type": "number"
    },
    "taxRate": {
      "type": "number"
    }
  },
  "required": ["currency", "pricePerKwh"]
}
